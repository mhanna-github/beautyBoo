---
import Wysiwyg from '@root/src/components/Wysiwyg/Wysiwyg.astro';
---

<section data-scroll-section>
    <h2 class="u-sr-only">reviezs</h2>
    <div class="u-container u-mt-[20vw] u-flex u-flex-col u-items-center u-bg-black u-text-white">
        <div class="swiper">
            <ul class="swiper-wrapper u-max-w-5xl">
                <li class="swiper-slide u-flex u-flex-col u-items-center u-gap-[3vw]">
                    <div class="u-flex u-flex-row u-gap-1">
                        {
                            [...Array(5)].map(() => (
                                <svg
                                    width="26"
                                    height="26"
                                    viewBox="0 0 26 26"
                                    fill="none"
                                    xmlns="http://www.w3.org/2000/svg"
                                    xmlns:xlink="http://www.w3.org/1999/xlink"
                                >
                                    <mask
                                        id="mask0_163_1131"
                                        style="mask-type:alpha"
                                        maskUnits="userSpaceOnUse"
                                        x="0"
                                        y="0"
                                        width="26"
                                        height="26"
                                    >
                                        <rect
                                            width="26"
                                            height="26"
                                            fill="url(#pattern0_163_1131)"
                                        />
                                    </mask>
                                    <g mask="url(#mask0_163_1131)">
                                        <rect
                                            y="-2.00049"
                                            width="28.6667"
                                            height="30"
                                            fill="#BADEF3"
                                        />
                                    </g>
                                    <defs>
                                        <pattern
                                            id="pattern0_163_1131"
                                            patternContentUnits="objectBoundingBox"
                                            width="1"
                                            height="1"
                                        >
                                            <use
                                                xlink:href="#image0_163_1131"
                                                transform="scale(0.00195312)"
                                            />
                                        </pattern>
                                        <image
                                            id="image0_163_1131"
                                            width="512"
                                            height="512"
                                            xlink:href="data:image/png;base64,"
                                        />
                                    </defs>
                                </svg>
                            ))
                        }
                    </div>
                    <Wysiwyg class="u-text-p u-normal-case">
                        <Fragment
                            set:html={`<p>I just wanted to say how impressed I am with your products. The results speak for themselves – my skin has never looked better! You've gained a loyal customer in me.</p>`}
                        />
                    </Wysiwyg>
                    <span class="">
                        <h4 class="u-text-p u-uppercase">Ophelia</h4>
