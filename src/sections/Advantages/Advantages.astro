---
import Image from '@components/Image/Image.astro';
---

<section data-scroll-section>
    <div class="u-container u-mt-[20vw] u-flex u-flex-col u-gap-gutter lg:u-flex-row">
        <div class="u-relative u-order-1 lg:u-order-2">
            <h2
                class="u-left-[-25%] u-top-[-8%] u-text-center u-text-h2 u-uppercase lg:u-absolute lg:u-text-left"
            >
                You should make us your beauty destination, <span
                    class="u-underline u-transition-all">cause</span
                >
            </h2>
            <ul
                class="u-mt-[8vw] u-flex u-flex-col u-gap-gutter u-text-center lg:u-ml-[4vw] lg:u-text-left"
            >
                <li class="u-flex u-flex-col u-items-center u-gap-3 lg:u-flex-row">
                    <svg
                        width="93"
                        height="72"
                        viewBox="0 0 93 72"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg"
                        xmlns:xlink="http://www.w3.org/1999/xlink"
                    >
                        <mask
                            id="mask0_117_933"
                            style="mask-type:alpha"
                            maskUnits="userSpaceOnUse"
                            x="0"
                            y="0"
                            width="93"
                            height="72"
                        >
                            <rect width="92.4" height="71.0769" fill="url(#pattern0_117_933)"
                            ></rect>
                        </mask>
                        <g mask="url(#mask0_117_933)">
                            <rect
                                x="-4.73853"
                                y="1.77686"
                                width="100.692"
                                height="69.3"
                                rx="34.65"
                                fill="#579ACD"></rect>
                        </g>
                        <defs>
                            <pattern
                                id="pattern0_117_933"
                                patternContentUnits="objectBoundingBox"
                                width="1"
                                height="1"
                            >
                                <use
                                    xlink:href="#image0_117_933"
                                    transform="matrix(0.00195312 0 0 0.00253906 0 -0.15)"></use>
                            </pattern>
                            <image
                                id="image0_117_933"
                                width="512"
                                height="512"
                                xlink:href="data:image/png;base64,"
                            />
                        </defs>
                    </svg>
                    <div>
                        <h3 class="u-text-h3 u-uppercase u-text-secondary">
                            For all skin & hair types
                        </h3>
                        <p class="u-text-p lg:u-max-w-lg">
                            Experience beauty without compromise as our hypoallergenic cosmetic
                            products are formulated with utmost care, free from common allergens and
                            irritants.
                        </p>
                    </div>
                </li>
                <li class="u-flex u-flex-col u-items-center u-gap-3 lg:u-flex-row">
                    <svg
                        width="93"
                        height="72"
                        viewBox="0 0 72 72"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg"
                        xmlns:xlink="http://www.w3.org/1999/xlink"
                    >
                        <mask
                            id="mask0_117_936"
                            style="mask-type:alpha"
                            maskUnits="userSpaceOnUse"
                            x="0"
                            y="0"
                            width="72"
                            height="72"
                        >
                            <rect width="71.08" height="71.08" fill="url(#pattern0_117_936)"></rect>
                        </mask>
                        <g mask="url(#mask0_117_936)">
                            <rect width="102.622" height="71.08" fill="#579ACD"></rect>
                        </g>
                        <defs>
                            <pattern
                                id="pattern0_117_936"
                                patternContentUnits="objectBoundingBox"
                                width="1"
                                height="1"
                            >
                                <use xlink:href="#image0_117_936" transform="scale(0.00195312)"
                                ></use>
                            </pattern>
                            <image
                                id="image0_117_936"
                                width="512"
                                height="512"
                                xlink:href="data:image/png;base64,"
                            />
                        </defs>
                    </svg>
                    <div>
                        <h3 class="u-text-h3 u-uppercase u-text-secondary">Hypoallergenic</h3>
                        <p class="u-text-p lg:u-max-w-lg">
                            Our carefully crafted products are free from common allergens and
                            irritants, suitable for all skin and hair types, solution for a flawless
                            and radiant look.
                        </p>
                    </div>
                </li>
                <li class="u-flex u-flex-col u-items-center u-gap-3 lg:u-flex-row">
                    <svg
                        width="93"
                        height="72"
                        viewBox="0 0 72 72"
                        fill="none"
                        xmlns="http://www.w3.org/2000/svg"
                        xmlns:xlink="http://www.w3.org/1999/xlink"
                    >
                        <mask
                            id="mask0_117_938"
                            style="mask-type:alpha"
                            maskUnits="userSpaceOnUse"
                            x="0"
                            y="0"
                            width="72"
                            height="72"
                        >
                            <rect width="71.08" height="71.08" fill="url(#pattern0_117_938)"></rect>
                        </mask>
                        <g mask="url(#mask0_117_938)">
                            <rect x="-3.33179" width="89.9606" height="74.4119" fill="#579ACD"
                            ></rect>
                        </g>
                        <defs>
                            <pattern
                                id="pattern0_117_938"
                                patternContentUnits="objectBoundingBox"
                                width="1"
                                height="1"
                            >
                                <use xlink:href="#image0_117_938" transform="scale(0.00195312)"
                                ></use>
                            </pattern>
                            <image
                                id="image0_117_938"
                                width="512"
                                height="512"
                                xlink:href="data:image/png;base64,"
                            />
                        </defs>
                    </svg>
                    <div>
                        <h3 class="u-text-h3 u-uppercase u-text-secondary">
                            Made of Natural Products
                        </h3>
                        <p class="u-text-p lg:u-max-w-lg">
                            Experience the exquisite blend of nature's goodness and beauty with our
                            cosmetic products, meticulously crafted from natural ingredients.
                        </p>
                    </div>
                </li>
            </ul>
        </div>
        <div class="u-relative u-order-2 lg:u-order-1 lg:u-w-1/2">
            <figure class="u-h-auto u-w-full">
                <Image
                    src={import('@images/Cream-drop.jpg')}
                    alt="Cream drop"
                    class="u-h-full u-w-full u-object-cover"
                />
            </figure>
            <h4
                class="u-absolute u-left-0 u-top-[30vw] u-z-20 u-text-center u-text-h4 u-uppercase lg:u-left-[2vw] lg:u-right-0 lg:u-text-left"
            >
                Our commitment to quality and your well-being is reflected in every aspect of our
                offerings.
            </h4>
        </div>
    </div>
</section>
